# Environment Variables for Smart Contract Deployment
# Copy this file to .env and fill in your values

# Network RPC URLs
SEPOLIA_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
MAINNET_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Wallet Private Key (NEVER commit the actual .env file)
PRIVATE_KEY=your_private_key_here

# Etherscan API Key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Contract Addresses (will be populated after deployment)
PROFITHIVE_TOKEN_ADDRESS=
REVENUE_SHARING_ADDRESS=
FORECAST_DATA_ADDRESS=